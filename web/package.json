{
  "name": "task-platform-frontend",
  "version": "1.0.0",
  "description": "任务交易平台前端应用",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:perf": "vue-tsc && vite build --config vite.config.performance.ts",
    "build:analyze": "ANALYZE=true vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.0",
    "@vueuse/core": "^11.0.0",
    "axios": "^1.7.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.0",
    "echarts": "^5.5.0",
    "element-plus": "^2.8.0",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.0",
    "qrcode": "^1.5.3",
    "vue": "^3.5.0",
    "vue-echarts": "^7.0.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.0.0",
    "@types/nprogress": "^0.2.3",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/ui": "^2.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^25.0.0",
    "lightningcss": "^1.31.1",
    "prettier": "^3.3.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass-embedded": "^1.97.3",
    "typescript": "~5.6.0",
    "unplugin-auto-import": "^0.18.0",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^6.4.1",
    "vite-plugin-compression2": "^1.4.0",
    "vitest": "^2.1.0",
    "vue-tsc": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
